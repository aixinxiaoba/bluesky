function loadCustomer()
{
	$.ajax({
		url : "/login/loadCustomer.do",
		type : "post",
		dataType : "json",
		async : true,
		success:function(data){
			if (data != null && data.lId != null && data.lId > 0)
			{
				$("#needLogin").hide();
				$("#allreadyLogin").show();
				$("#nickName").html(data.strNickName);
			}
			else
			{
				$("#needLogin").show();
			}
		}
	});
}
	
